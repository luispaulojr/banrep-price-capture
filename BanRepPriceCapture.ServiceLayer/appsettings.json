{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DtfDailyCapture": {
    "QueueName": "dtf-daily",
    "CsvDirectory": "artifacts/dtf-daily",
    "PersistenceBatchSize": 200,
    "PersistenceParallelism": 4,
    "RequeueNotificationThreshold": 3
  },
  "Database": {
    "DatabaseName": "banrep",
    "Port": 5432,
    "EnableSsl": false
  },
  "DatabaseSecrets": {
    "SecretId": "lbanrep",
    "HostEnvVar": "BANREP_DB_HOST",
    "HostReadOnlyEnvVar": "BANREP_DB_HOST_RO",
    "UsernameEnvVar": "BANREP_DB_USERNAME",
    "PasswordEnvVar": "BANREP_DB_PASSWORD"
  },
  "RabbitMq": {
    "HostName": "localhost",
    "Port": 5672,
    "VirtualHost": "/",
    "UserNameEnvVar": "BANREP_RABBITMQ_USERNAME",
    "PasswordEnvVar": "BANREP_RABBITMQ_PASSWORD"
  },
  "ExternalServices": {
    "Notification": {
      "BaseUrl": "",
      "Enabled": true,
      "SystemId": "BanRepPriceCapture.Dtf",
      "TimeoutSeconds": 10,
      "Retry": {
        "MaxAttempts": 3,
        "BackoffSeconds": 2
      }
    },
    "Sdmx": {
      "BaseUrl": "https://totoro.banrep.gov.co/nsi-jax-ws/rest/data/",
      "TimeoutSeconds": 30
    },
    "DtfDailyOutbound": {
      "BaseUrl": "https://example.com/dtf-daily",
      "Authentication": {
        "TokenEnvVar": "BANREP_BEARER_TOKEN"
      }
    },
    "S3Artifacts": {
      "BucketName": "banrep-price-capture-artifacts-local",
      "Environment": "local",
      "Retry": {
        "MaxAttempts": 3,
        "BackoffSeconds": 2
      }
    }
  },
  "AWS": {
    "Region": "us-east-1"
  },
  "AWSLogging": {
    "LogGroup": "banrep-price-capture",
    "LogStreamNamePrefix": "dtf-weekly",
    "Region": "us-east-1"
  }
}
